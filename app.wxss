/**app.wxss**/
@import '/static/css/font-awesome.min.wxss';
@import '/static/css/stylesheet.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}
.page-head {
  background-color: aqua;
  width: 100%;
  height: 32pt;
  display: flex;
  flex-direction: row;
}
.page-head-title {
  line-height: 32pt;
  padding-left: 20rpx;
  font-weight: bold;
}
.content {
  margin-top: 32pt;
  width: 100%;
  height: 100vh;
  background-color: blueviolet;
  display: flex;
  flex-direction: column;
}

